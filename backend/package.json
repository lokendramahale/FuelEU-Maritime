    "ts-jest": "^29.1.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.42.0",
    "@types/node": "^20.5.1"
